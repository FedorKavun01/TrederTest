<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/LL"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="30sp"
                android:textAlignment="center"
                android:text="Hello, world!"
                android:layout_marginBottom="100dp"/>
            <TableLayout
                android:layout_width="300dp"
                android:layout_height="300dp">
                <TableRow>
                    <TextView
                        android:id="@+id/sqr11"
                        android:width="100dp"
                        android:height="100dp"/>
                    <TextView
                        android:id="@+id/sqr12"
                        android:width="100dp"
                        android:height="100dp"
                        android:background="@drawable/horizontal_textlines"/>
                    <TextView
                        android:id="@+id/sqr13"
                        android:width="100dp"
                        android:height="100dp"/>
                </TableRow>
                <TableRow>
                    <TextView
                        android:id="@+id/sqr21"
                        android:width="100dp"
                        android:height="100dp"
                        android:background="@drawable/vertical_textlines"/>
                    <TextView
                        android:id="@+id/sqr22"
                        android:width="100dp"
                        android:height="100dp"
                        android:background="@drawable/all_textlines"/>
                    <TextView
                        android:id="@+id/sqr23"
                        android:width="100dp"
                        android:height="100dp"
                        android:background="@drawable/vertical_textlines"/>
                </TableRow>
                <TableRow>
                    <TextView
                        android:id="@+id/sqr31"
                        android:width="100dp"
                        android:height="100dp" />
                    <TextView
                        android:id="@+id/sqr32"
                        android:width="100dp"
                        android:height="100dp"
                        android:background="@drawable/horizontal_textlines"/>
                    <TextView
                        android:id="@+id/sqr33"
                        android:width="100dp"
                        android:height="100dp" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </WebView>


</androidx.constraintlayout.widget.ConstraintLayout>